/**app.wxss**/
.container {
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  position: relative;
}

page {
  background: #f4f4f4;
}


/* s加载中 */


.loadBtn {
  background: transparent;
  letter-spacing: 5rpx;
  padding: 10rpx 0;
  color: #666;
}

.loadBtn[loading]::before {
  margin-right: 8rpx;
}

.loadBtn::after {
  display: none;
}

.box-b {
  box-sizing: border-box;
}

/*checkbox  */

checkbox {
  vertical-align: middle;
}

checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #7972a9;
  color: #fff !important;
  border: 1px solid #7972a9;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  font-size: 25rpx;
}

