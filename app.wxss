}

.chatTriangleLeftTop {
  width: 0;
  height: 0;
  display: block;
  border-bottom: 2vw solid transparent;
  border-top: 2vw solid transparent;
  border-right: 2vw solid #fff;
  position: absolute;
  top: 1vw;
  left: -1.8vw;
}

.chatTriangleRightTop {
  width: 0;
  height: 0;
  display: block;
  border-bottom: 2vw solid transparent;
  border-top: 2vw solid transparent;
  border-left: 2vw solid #9ee65f;
  position: absolute;
  top: 2vw;
  right: -1.8vw;
}

/*
 * 行样式
 */

.lineItem {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  padding: 2%;
  position: relative;
  background: #fff;
}

.lineItem.border::after {
  content: " ";
  display: block;
  width: 100%;
  position: absolute;
  bottom: -3rpx;
  right: 0;
  height: 1px;
  border-top: 1px solid #d9d9d9;
  color: #d9d9d9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 2;
}

.lineItem .left {
  text-align: center;
  display: flex;
}

.lineItem .twoLine {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.lineItem .left text.iconfont {
  margin-right: 20rpx;
}

.lineItem .center {
  flex: 1;
  margin-right: auto;
  text-align: left;
  display: flex;
  flex-direction: column;
  justify-content: center;
  flex-wrap: wrap;
}

.lineItem .right {
  margin-left: auto;
  text-align: right;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
}

.lineItem_access .right {
  padding-right: 28rpx;
}

.lineItem_access .right::after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -4px;
  right: 2px;
}

/*
 * 带输入框行样式
 */

.itemInput {
  display: flex;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  color: #000;
  background: #fff;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 2% 0 2%;
  position: relative;
  border-bottom: 1px solid #f4f4f4;
}

.itemInput .itemleft {
  width: 500rpx;
}

.itemInput .itemcenter {
  width: 100%;
  padding-right: 150rpx;
  display: flex;
}

.itemInput .itemcenter input {
  width: 100%;
  height: 30px;
  margin-top: 10px;
  font-size: 14px;
}

.itemInput .itemright {
  position: absolute;
  right: 0;
  width: 150rpx;
  height: 50px;
  padding-right: 10rpx;
}

.flex {
  display: flex;
}

.flexMiddle {
  display: flex;
  align-items: center;
}

.flex .flex-item {
  flex: 1;
}

.borderA:after {
  content: " ";
  display: block;
  width: 100%;
  position: absolute;
  bottom: -3rpx;
  right: 0;
  height: 1px;
  border-top: 1px solid #d9d9d9;
  color: #d9d9d9;
  -webkit-transform-origin: 0 0;
