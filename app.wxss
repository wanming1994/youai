.border_right{
  position: relative;
}
.border_right:after {
  content: "";
  display: block;
  width: 1px;
  height: 200%;
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  background: #d9d9d9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 2;
}

